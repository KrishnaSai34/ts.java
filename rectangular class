class rect
{
int l=2;
int b=3;
int a=l*b;
int p=2*(l+b);

void area()
{

 System.out.println("teh area is :"+a);

}

void per()
{

System.out.println("the per is :"+p);
}

 public static void  main(String args[])
{
rect fobj =new rect();
System.out.println(fobj.a);
System.out.println(fobj.p);
fobj.area();
fobj.per();
}
}
